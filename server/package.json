{
    "name": "cherry-blossom-tracker-server",
    "version": "1.0.0",
    "description": "Server for Vancouver Cherry Blossom Tracker",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node-dev src/index.ts",
