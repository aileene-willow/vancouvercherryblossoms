    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.0",
        "@types/pg": "^8.10.9",
        "@types/supertest": "^6.0.3",
        "jest": "^29.7.0",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.3",
        "vercel": "32.0.1"
